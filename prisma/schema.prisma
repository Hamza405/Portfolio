// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Skill {
  id String @id @default(cuid())
  skillName String
  image String
  basic Boolean
}

model Project {
  id String @id @default(cuid())
  projectName String
  projectDescription String
  projectImages String[]
}
